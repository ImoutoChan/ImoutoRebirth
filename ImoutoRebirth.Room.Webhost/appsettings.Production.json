{
  "ConnectionStrings": {
    "RoomDatabase": "#RoomConnectionString"
  },
  "RabbitSettings": {
    "Url": "#RabbitHost",
    "Username": "#RabbitUsername",
    "Password": "#RabbitPassword"
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "#ElasticUri",
          "indexFormat": "#ElasticIndexFormat",
          "restrictedToMinimumLevel": "#ElasticMinimumLogLevel"
        }
      }
    ]
  }
}