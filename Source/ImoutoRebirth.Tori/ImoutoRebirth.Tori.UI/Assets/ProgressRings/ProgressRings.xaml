<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DrawingGroup x:Key="RingDrawingGroup" ClipGeometry="M0,0 V27 H27 V0 H0 Z">
        <DrawingGroup Opacity="1">
            <DrawingGroup Opacity="1">
                <DrawingGroup Opacity="1">
                    <GeometryDrawing Brush="#FFD5AB66">
                        <GeometryDrawing.Geometry>
                            <EllipseGeometry RadiusX="6.5" RadiusY="6.5" Center="13.5,13.5" />
                        </GeometryDrawing.Geometry>
                    </GeometryDrawing>
                    <GeometryDrawing>
                        <GeometryDrawing.Pen>
                            <Pen Brush="#FFD5AB66" Thickness="2" StartLineCap="Flat" EndLineCap="Flat" LineJoin="Miter" />
                        </GeometryDrawing.Pen>
                        <GeometryDrawing.Geometry>
                            <EllipseGeometry RadiusX="12.5" RadiusY="12.5" Center="13.5,13.5" />
                        </GeometryDrawing.Geometry>
                    </GeometryDrawing>
                </DrawingGroup>
            </DrawingGroup>
        </DrawingGroup>
    </DrawingGroup>

    <DrawingImage x:Key="RingDrawingImage" Drawing="{StaticResource RingDrawingGroup}" />

    <DrawingGroup x:Key="BarDrawingGroup" ClipGeometry="M0,0 V6 H38 V0 H0 Z">
        <DrawingGroup Opacity="1">
            <DrawingGroup Opacity="1">
                <GeometryDrawing Brush="#FFD5AB66">
                    <GeometryDrawing.Pen>
                        <Pen Brush="#FFD5AB66" Thickness="1" StartLineCap="Flat" EndLineCap="Flat" LineJoin="Miter" />
                    </GeometryDrawing.Pen>
                    <GeometryDrawing.Geometry>
                        <RectangleGeometry RadiusX="2" RadiusY="2" Rect="0.5,0.5,37,5" />
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingGroup>
    </DrawingGroup>

    <DrawingImage x:Key="BarDrawingImage" Drawing="{StaticResource BarDrawingGroup}" />

    <DrawingGroup x:Key="RingDrawingGroup.Gray" ClipGeometry="M0,0 V27 H27 V0 H0 Z">
        <DrawingGroup Opacity="1">
            <DrawingGroup Opacity="1">
                <DrawingGroup Opacity="1">
                    <GeometryDrawing Brush="#D9D9D9">
                        <GeometryDrawing.Geometry>
                            <EllipseGeometry RadiusX="6.5" RadiusY="6.5" Center="13.5,13.5" />
                        </GeometryDrawing.Geometry>
                    </GeometryDrawing>
                    <GeometryDrawing>
                        <GeometryDrawing.Pen>
                            <Pen Brush="#D9D9D9" Thickness="2" StartLineCap="Flat" EndLineCap="Flat" LineJoin="Miter" />
                        </GeometryDrawing.Pen>
                        <GeometryDrawing.Geometry>
                            <EllipseGeometry RadiusX="12.5" RadiusY="12.5" Center="13.5,13.5" />
                        </GeometryDrawing.Geometry>
                    </GeometryDrawing>
                </DrawingGroup>
            </DrawingGroup>
        </DrawingGroup>
    </DrawingGroup>

    <DrawingImage x:Key="RingDrawingImage.Gray" Drawing="{StaticResource RingDrawingGroup.Gray}" />

    <DrawingGroup x:Key="BarDrawingGroup.Gray" ClipGeometry="M0,0 V6 H38 V0 H0 Z">
        <DrawingGroup Opacity="1">
            <DrawingGroup Opacity="1">
                <GeometryDrawing Brush="#D9D9D9">
                    <GeometryDrawing.Pen>
                        <Pen Brush="#D9D9D9" Thickness="1" StartLineCap="Flat" EndLineCap="Flat" LineJoin="Miter" />
                    </GeometryDrawing.Pen>
                    <GeometryDrawing.Geometry>
                        <RectangleGeometry RadiusX="2" RadiusY="2" Rect="0.5,0.5,37,5" />
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingGroup>
    </DrawingGroup>

    <DrawingImage x:Key="BarDrawingImage.Gray" Drawing="{StaticResource BarDrawingGroup.Gray}" />
</ResourceDictionary>