<Application x:Class="ImoutoRebirth.Tori.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ImoutoRebirth.Tori.UI"
             xmlns:converters="clr-namespace:ImoutoRebirth.Tori.UI.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converters:BooleanToStatusConverter x:Key="BooleanToStatusConverter"/>
        <converters:InverseBooleanConverter x:Key="InverseBooleanConverter"/>
        <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
        <converters:InverseBooleanToVisibilityConverter x:Key="InverseBooleanToVisibilityConverter"/>
    </Application.Resources>
</Application> 