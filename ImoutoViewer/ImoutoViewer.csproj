<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Company>ImoutoRebirth</Company>
    <Product>Imouto Viewer</Product>
    <Copyright>ImoutoChan ©  2013-2020</Copyright>
    <AssemblyVersion>2.0.0</AssemblyVersion>
    <FileVersion>2.0.0</FileVersion>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <PropertyGroup>
    <ApplicationIcon>Resources\49a7b0a953e5106e5942169f3cb0c237_ copy.ico</ApplicationIcon>
  </PropertyGroup>

  <PropertyGroup>
    <StartupObject>ImoutoViewer.App</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Imouto.SharedModelLibrary">
      <HintPath>..\..\imouto-library\bin\Release\Imouto.SharedModelLibrary.dll</HintPath>
    </Reference>
    <Reference Include="Imouto.Utils">
      <HintPath>..\..\imouto-library\bin\Release\Imouto.Utils.dll</HintPath>
    </Reference>
    <Reference Include="Imouto.WCFExchageLibrary">
      <HintPath>..\..\imouto-library\bin\Release\Imouto.WCFExchageLibrary.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
      <SubType>Designer</SubType>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\img\nothing.png" />
    <Resource Include="Resources\49a7b0a953e5106e5942169f3cb0c237_ copy.ico" />
    <Resource Include="Resources\img\edit\irfan_view.ico" />
    <Resource Include="Resources\img\edit\ms_paint.ico" />
    <Resource Include="Resources\img\edit\photoshop.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MahApps.Metro" Version="2.0.0" />
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="rfc1459.AssociationManager" Version="1.1.1" />
    <PackageReference Include="WpfAnimatedGif" Version="2.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>
</Project>